<?xml version="1.0" encoding="utf-8" ?>
<Policy xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Project></Project> <!-- project name in Jira, e.g. BPRO -->
  <BaseUrl></BaseUrl> <!-- url address of project in Jira, e.g. https://bpro.atlassian.net -->
  <Username></Username> <!-- username to access Jira -->
  <Password></Password> <!-- password of username to access Jira -->
  <PermanentTaskLabel></PermanentTaskLabel> <!-- label of permanent tasks in Jira, e.g. permanent -->
  <ReopenedStatus></ReopenedStatus> <!-- label of reopened tasks in Jira, e.g. reopened -->
  <RootPath></RootPath> <!-- e.g. BoardProspects -->
  <IsWeekendReportActive></IsWeekendReportActive> <!-- runs report for weekend on mondays, e.g true -->
  <AllocatedHoursPerMonth></AllocatedHoursPerMonth> <!-- e.g 400 -->
  <TargetGroup></TargetGroup> <!-- target group, e.g. BoardProspects -->
  <Emails></Emails> <!-- e-mail address of the recipient, e.g. contact@equilobe.com -->
  <EmailSubject></EmailSubject> <!-- e-mail subject, e.g. Daily Report for -->
  <ReportTitle></ReportTitle> <!-- e-mail report title, e.g. Board Prospects -->
  <SourceControl>
    <Type></Type> <!-- type of the Source Control System, e.g. SVN, GitHub -->
    <RepoOwner></RepoOwner> <!-- owner of the repository, e.g. EQUILOBE -->
		<RepoName></RepoName> <!-- name of the repository -->
		<RepoUrl></RepoUrl> <!-- url address of the repository, e.g. svn://10.0.0.1/repos -->
    <Username></Username> <!-- username to access repository -->
    <Password></Password> <!-- password of username to access repository -->
    <CommitUrl></CommitUrl> <!-- URL for commits tool -->
  </SourceControl>
  <AuthorsCorrelation>
    <User>
      <JiraAuthor></JiraAuthor> <!-- name of user on Jira, e.g. John Doe -->
      <SourceControlAuthor></SourceControlAuthor> <!-- name of user on source control system, e.g. john.doe -->
    </User>
  </AuthorsCorrelation>
</Policy>
