@if (Model.Count > 0)
{
    <span style="color:#ABABAB">
        @Model.Count
        @if (Model.Count == 1)
        {
            <span style="color:#ABABAB;"> commit </span>
        }
        else
        {
            <span style="color:#ABABAB;"> commits </span>
        }
        @Partial("Views/commitsList.cshtml", Model)
    </span>
}