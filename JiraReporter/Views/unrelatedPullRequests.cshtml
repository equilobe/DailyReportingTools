@{
    var comma = ",";
}
@if(Model.Count>0)
{
    foreach (var pullRequest in Model)
    {
        if (pullRequest == Model.Last())
        {
            comma = "";
        }
        else
        {
            comma = ", ";
        }
        <span style="color:black; font-size:14px;"><a style="text-decoration:none; color:green" href="@pullRequest.GithubPullRequest.HtmlUrl">@pullRequest.GithubPullRequest.Number</a>@comma</span>
    }
}
